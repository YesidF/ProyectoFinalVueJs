ACA;AAZA,GAnBA;AAiCA,EAAA,IAjCA,kBAiCA;AACA,WAAA;AACA,MAAA,MAAA,EAAA,cADA;AAEA,MAAA,OAAA,EAAA,oBAFA;AAGA,MAAA,IAAA,EAAA,EAHA;AAIA,MAAA,QAAA,EAAA,CAAA,QAAA,EAAA,UAAA,EAAA,YAAA,EAAA,WAAA,CAJA;AAKA,MAAA,MAAA,EAAA,CACA;AACA,QAAA,MAAA,EAAA,2BADA;AAEA,QAAA,IAAA,EAAA,MAFA;AAGA,QAAA,OAAA,EAAA,CAAA,gBAAA,EAAA,kBAAA,EAAA,uBAAA,CAHA;AAIA,QAAA,GAAA,EAAA;AAJA,OADA,EAOA;AACA,QAAA,MAAA,EAAA,0BADA;AAEA,QAAA,IAAA,EAAA,MAFA;AAGA,QAAA,OAAA,EAAA,CAAA,iBAAA,CAHA;AAIA,QAAA,GAAA,EAAA;AAJA,OAPA,EAaA;AACA,QAAA,MAAA,EAAA,0CADA;AAEA,QAAA,IAAA,EAAA,MAFA;AAGA,QAAA,OAAA,EAAA,CAAA,kBAAA,CAHA;AAIA,QAAA,GAAA,EAAA;AAJA,OAbA,EAmBA;AACA,QAAA,MAAA,EAAA,2BADA;AAEA,QAAA,IAAA,EAAA,MAFA;AAGA,QAAA,OAAA,EAAA,CAAA,mBAAA,CAHA;AAIA,QAAA,GAAA,EAAA;AAJA,OAnBA,EAyBA;AACA,QAAA,MAAA,EAAA,sBADA;AAEA,QAAA,IAAA,EAAA,MAFA;AAGA,QAAA,OAAA,EAAA,CAAA,aAAA,CAHA;AAIA,QAAA,GAAA,EAAA;AAJA,OAzBA,CALA;AAqCA,MAAA,QAAA,EAAA;AArCA,KAAA;AAuCA;AAzEA,CAAA","sourcesContent":["<template>\r\n    <div id=\"form-usu\" class=\"mt-3\">\r\n        <div v-if=\"fullName\">\r\n            <h2 v-html=\"fullName\"></h2>\r\n        </div>\r\n        <form action=\"\" class=\"p-3\">\r\n            <h1 v->Registrar usuario.</h1>\r\n            <div class=\"mb-3\">Por favor, ingrese los siguentes datos.</div> \r\n\r\n            <div class=\"form-floating mb-3\">\r\n                <input v-model=\"nombre\" type=\"text\" class=\"form-control\" id=\"nombre\" name=\"nombre\" placeholder=\"Ingrese su estatura\">\r\n                <label for=\"nombre\">Nombre:</label>\r\n            </div>\r\n            <small>Mi nombre es: {{nombre}}</small><br>\r\n            <small>{{$data}}</small>\r\n            <div class=\"form-floating mb-3\">\r\n                <input v-model=\"edad\" type=\"number\" class=\"form-control\" id=\"edad\" name=\"edad\" placeholder=\"Ingrese su edad\">\r\n                <label for=\"apellido\">Edad:</label>\r\n            </div>\r\n            <small>Mi edad es: {{edad|concatena('con  parametro')|mayusculas}}</small>\r\n            <p style=\"color:red\" v-if=\"edad && edad >= 18 && edad <60\">Hola!! eres mayor de edad</p>\r\n            <p style=\"color:orange\" v-else-if=\"edad && edad >= 60\">Hola!! eres de avanzada edad</p>\r\n            <p style=\"color:green\" v-else>Hola!! eres  menor de edad</p>            \r\n            <div class=\"d-grid d-md-flex justify-content-md-end\">\r\n                <button type=\"submit\" class=\"btn btn-primary btn-lg\">Registrar</button>\r\n            </div>\r\n        </form>\r\n\r\n        <ol>\r\n            <li v-for=\"deporte in deportes\" :key=\"deporte\">{{deporte}}</li>\r\n        </ol>\r\n        <hr>\r\n        <h1 v-if=\"favorito\">El libro selecionado es {{favorito.titulo}}</h1>\r\n        <div v-for=\"libro in librosMayus\" :key=\"libro.titulo\">\r\n            <!--<Libro :libro=\"libro\" @favorita=\"ha_llegado_el_libro\"></Libro>-->\r\n            <Libro :libro=\"libro\" v-on:favorita=\"ha_llegado_el_libro\"></Libro>   \r\n        </div>\r\n        <small>{{favorito}}</small>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import Libro from './ComponenteHijo.vue';\r\n    export default {\r\n        name: 'DirectivasCondicionales',\r\n        components: {\r\n            Libro\r\n        },\r\n        methods:{\r\n            ha_llegado_el_libro(libroFav){\r\n                console.log(libroFav);\r\n                this.favorito=libroFav\r\n            }\r\n        },\r\n        filters:{\r\n            mayusculas(value){\r\n                return value.toUpperCase();\r\n            },\r\n            concatena(value, msg){\r\n                return value+' '+' texto concatenado '+ msg;\r\n            }\r\n        },\r\n        computed:{\r\n            //el nobre de la función es el que se implenta o se da uso en el componente\r\n            librosMayus(){\r\n                var librosMayus = this.libros;\r\n                for(var i = 0; i < librosMayus.length; i++){\r\n                    librosMayus[i].titulo = librosMayus[i].titulo.toUpperCase(); \r\n                }\r\n                return librosMayus;\r\n            },\r\n            fullName(){\r\n                var misDatos = this.nombre + '<br>' + this.apelido;\r\n                return misDatos;\r\n            }\r\n        },\r\n        data(){\r\n            return{\r\n                nombre: 'Dorian Yesid',\r\n                apelido: 'Fernández Estrella',\r\n                edad: 10,\r\n                deportes:['futbol','ciclismo','basketball','atletismo'],\r\n                libros:[\r\n                    {\r\n                        titulo: 'Psicológicamente hablando',\r\n                        anio: '2016',\r\n                        autores: ['Adrián Triglia', 'Bertrand Regader', 'Jonathan García-Allen'],\r\n                        img: 'https://pymstatic.com/3900/conversions/psicologicamente-hablando-default.jpg'\r\n                    },\r\n                    {\r\n                        titulo: 'Historia de dos ciudades',\r\n                        anio: '1859',\r\n                        autores: ['Charles Dickens'],\r\n                        img: 'https://pymstatic.com/1753/conversions/historia-dos-ciudades-default.jpg'\r\n                    },\r\n                    {\r\n                        titulo: '7 hábitos de la gente altamente efectiva',\r\n                        anio: '1859',\r\n                        autores: ['Stephen R. Covey'],\r\n                        img: 'https://m.media-amazon.com/images/I/517buCKnBjL._SY346_.jpg'\r\n                    },\r\n                    {\r\n                        titulo: 'Lo que el viento se llevó',\r\n                        anio: '1936',\r\n                        autores: ['Margaret Mitchell'],\r\n                        img: 'https://images.cdn2.buscalibre.com/fit-in/360x360/0d/51/0d513cf67ac3461fd0da21c25efb1a5a.jpg'\r\n                    },\r\n                    {\r\n                        titulo: 'El nombre de la rosa',\r\n                        anio: '1980',\r\n                        autores: ['Umberto Eco'],\r\n                        img: 'https://images.cdn2.buscalibre.com/fit-in/360x360/44/51/4451db7bb092a71eb16850488d5f67e0.jpg'\r\n                    }\r\n                ],\r\n                favorito: \"\"\r\n            }\r\n        }\r\n    }\r\n</script>"],"sourceRoot":"src/components"}]}